}else if(${count_1}==1){
	StringBuffer buffer = new StringBuffer();
	buffer.append(${uploadId_1});
	buffer.append(&quot;,&quot;);
	vars.put(&quot;UPID&quot;,buffer.toString());
	log.info(&quot;参数：&quot;+vars.get(&quot;UPID&quot;));
}else if($(count_1)==2){
	StringBuffer buffer = new StringBuffer();
	buffer.append(${uploadId_1});
	buffer.append(&quot;,&quot;);
	buffer.append(${uploadId_2});
	buffer.append(&quot;,&quot;);
	vars.put(&quot;UPID&quot;,buffer.toString());
	log.info(&quot;参数：&quot;+vars.get(&quot;UPID&quot;));
